{
    "functions": {
      "GameMaster/wsgi.py": {
        "runtime": "@vercel/python@3.9"
      }
    },
    "build": {
      "env": {
        "PYTHON_VERSION": "3.9",
        "PIP_NO_BINARY": "psycopg2"
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "GameMaster/wsgi.py"
      }
    ]
  }